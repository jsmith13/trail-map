CREATE TABLE public.trails
(
    hike_id integer NOT NULL,
    ascent integer,
    descent integer,
    difficulty character varying(2000) COLLATE pg_catalog."default",
    high integer,
    latitude character varying(2000) COLLATE pg_catalog."default",
    distance character varying(2000) COLLATE pg_catalog."default",
    city character varying(2000) COLLATE pg_catalog."default",
    longitude character varying(2000) COLLATE pg_catalog."default",
    low integer,
    nickname character varying(2000) COLLATE pg_catalog."default",
    vote_count character varying(200) COLLATE pg_catalog."default",
    rating character varying(200) COLLATE pg_catalog."default",
    summary character varying(2000) COLLATE pg_catalog."default",
    url character varying(2000) COLLATE pg_catalog."default",
    CONSTRAINT trails_pkey PRIMARY KEY (hike_id)
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE public.trails
    OWNER to postgres;
